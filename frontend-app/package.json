{
  "name": "frontend-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5173",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test -c source/playwright/config/playwright.e2e.config.js",
    "test:e2e:ui": "playwright test --ui -c source/playwright/config/playwright.e2e.config.js",
    "test:e2e:headed": "playwright test --headed -c source/playwright/config/playwright.e2e.config.js",
    "test:e2e:debug": "PWDEBUG=1 playwright test -c source/playwright/config/playwright.e2e.config.js",
    "test:ct": "playwright test -c source/playwright/config/playwright.ct.config.js",
    "test:ct:ui": "playwright test --ui -c source/playwright/config/playwright.ct.config.js",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.vue"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/vue3": "^6.1.17",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@vee-validate/zod": "^4.15.0",
    "axios": "^1.10.0",
    "dayspan-vuetify": "^0.4.0",
    "luxon": "^3.7.1",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.13",
    "vue-i18n": "10",
    "vue-router": "4",
    "vuetify": "^3.8.6",
    "yup": "^1.6.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@eslint/js": "^9.34.0",
    "@pinia/testing": "^1.0.2",
    "@playwright/experimental-ct-vue": "^1.55.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "vite": "^6.2.0",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  }
